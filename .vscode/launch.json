{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "C/C++: gcc 활성 파일 디버그",
            "type": "cppdbg",
            "request": "launch",
            // "program": "${fileDirname}/a.out",
            "program": "${workspaceFolder}/push_swap/push_swap",
            "args": [333 153 458 187 100 367 485 0 276 58 162 5 145 424 393 345 101 466 464 474 196 299 84 200 441 449 125 142 401 385 52 36 44 329 19 41 344 253 1 229 457 289 262 292 112 263 309 339 312 136 483 70 297 158 124 265 137 152 29 96 258 407 443 213 139 55 205 381 446 134 16 480 127 243 478 496 204 382 143 268 305 300 330 396 157 484 130 460 246 73 105 332 60 451 210 121 179 62 220 389],
            "stopAtEntry": false,
            // "cwd": "${fileDirname}",
            "cwd": "${workspaceFolder}/push_swap/",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Enable multi-threading",
                    "text": "set print thread-events on",
                    "ignoreFailures": true
                }
            ],
            "logging": {
                "engineLogging": true,
                "trace": true,
                "traceResponse": true
            }
        }
    ]
}
